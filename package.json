{
  "name": "stacklogger",
  "version": "1.0.0",
  "description": "Colored JavaScript logging library that prepends the current class and function name to the message for ease of debugging.",
  "main": "distribution/index.js",
  "scripts": {
    "build": "babel source --presets babel-preset-es2015 --out-dir distribution --ignore example.js",
    "prepublish": "npm run build",
    "buildExample": "webpack --display-error-details --progress"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "log",
    "logger",
    "javascript",
    "stack",
    "caller",
    "color"
  ],
  "author": "Christoph Michel",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0"
  }
}
